{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "workbench.editor.tabSizing": "shrink",
  "git.suggestSmartCommit": false,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.smartCommitChanges": "tracked",
  "gitlens.views.remotes.branches.layout": "tree",
  "eslint.lintTask.enable": true,
  "conventionalCommits.scopes": ["project-structure", "project", "special-file", "thanks", "optimizing"],
  "files.eol": "\n",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css, scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#fdfdfe",
    "activityBar.background": "#fdfdfe",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#d4aaaa",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#fdfdfe",
    "statusBar.background": "#e1e3e7",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#c5c9d0",
    "statusBarItem.remoteBackground": "#e1e3e7",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#e1e3e7",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#e1e3e799",
    "titleBar.inactiveForeground": "#15202b99",
    "tab.activeBorder": "#fdfdfe"
  },
  "peacock.color": "#e1e3e7"
}
